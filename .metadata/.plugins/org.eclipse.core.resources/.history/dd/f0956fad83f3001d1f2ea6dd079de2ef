package study03.calculator;

import java.util.Scanner;

public class Add {
	
	int a;
	int b;
	
	
	public void setValue(int a, int b) {
		
		this.a = a;
		this.b = b;
		
	}
	
	public int calculate() {
		
		Scanner scan = new Scanner(System.in);
		
		this.a = scan.nextInt();
		
		return a + b;
		
	}

}
