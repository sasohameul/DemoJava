package study03;

public class Cat extends Animal {

	private String sound;

	public Cat(String sound) {
		super(sound);
	}

	
	@Override
	public void makeSound() {
		
		System.out.println(sound + "동물 소리를 재생합니다.");
	}


	public String getSound() {
		return sound;
	}


	public void setSound(String sound) {
		this.sound = sound;
	}
	
	
	
	
	
}
