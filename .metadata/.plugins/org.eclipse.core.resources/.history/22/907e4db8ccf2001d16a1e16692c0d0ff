package study03;

public class Duck extends Animal{


	public Duck() {
		super("꽥꽥");
	}


	@Override
	public void makeSound() {

		System.out.println(super.getSound() + "~ 동물 소리를 재생합니다.");

	}

	public String getsound() {
		return super.getSound();
	}

	public void setsound(String sound) {
	}



}
