package day14.api.ramda.basic;

public class MainClass {
	
	public static void main(String[] args) {
		
		//car의 run을 사용할 때마다 매번 다른기능으로 사용해야한다면 =>익명클래스는 장점을 갖습니다. 익명클래스 사용!
		Car car = new Car() {
			
			@Override
			public void run() {

				System.out.println("익명 객체로 생성된 car"); 
				
			}
		};
		
		car.run();
		
		
		
		
		
	}

}
